      <Button fx:id="back2" layoutX="425.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleBackAction2" prefHeight="37.0" prefWidth="67.0" text="Back" />
      <HBox layoutX="124.0" layoutY="169.0" prefHeight="183.0" prefWidth="261.0">
         <children>
            <ListView fx:id="numbers" prefHeight="183.0" prefWidth="139.0" />
            <ScrollBar orientation="VERTICAL" />
            <ListView fx:id="inputDisplay" prefHeight="183.0" prefWidth="135.0" />
         </children>
      </HBox>
      <Label fx:id="errorMessage1" layoutX="128.0" layoutY="360.0" prefHeight="17.0" prefWidth="261.0" textFill="RED" />
      <Label alignment="CENTER" layoutX="128.0" layoutY="148.0" prefHeight="17.0" prefWidth="115.0" text="Input Number" />
      <Label alignment="CENTER" layoutX="264.0" layoutY="148.0" prefHeight="17.0" prefWidth="115.0" text="Input" />
   </children>
</AnchorPane>
